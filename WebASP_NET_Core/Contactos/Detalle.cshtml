@page
@model Agenda.Pages.Contactos.DetalleModel
@{
	ViewData["Title"] = "Detalle de Contacto";
}

<div class="container">
	<div class="row">
		<div class="col-8 offset-2">
			<h2 class="text-info">Detalle de la Categoria: @Html.DisplayFor(m => m.Contacto.Id)</h2>
			<ul class="list-group">
				<li class="list-group-item"><strong>ID: </strong>@Html.DisplayFor(m => m.Contacto.Id)</li>
				<li class="list-group-item"><strong>Nombre: </strong>@Html.DisplayFor(m => m.Contacto.Nombre)</li>
				<li class="list-group-item"><strong>Nombre: </strong>@Html.DisplayFor(m => m.Contacto.Email)</li>
				<li class="list-group-item"><strong>Nombre: </strong>@Html.DisplayFor(m => m.Contacto.Telefono)</li>
				<li class="list-group-item"><strong>Nombre: </strong>@Html.DisplayFor(m => m.Contacto.Categoria.Nombre)</li>
				<li class="list-group-item"><strong>Fecha Creacion: </strong>@Html.DisplayFor(m => m.Contacto.FechaCreacion)</li>
			</ul>
		</div>
	</div>
	<div class="row">
		<div class="col-6">
			<a asp-page="Index" class="btn btn-success">Volver</a>
		</div>
	</div>
</div>